<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFC400"
    tools:context=".fin_entrenamiento">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="337dp"
        android:layout_height="121dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="37dp"
        android:layout_marginLeft="37dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:shadowColor="@color/black"
        android:text="ENTRENAMIENTO FINALIZADO"
        android:textColor="#FFFFFF"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/resultados_entrenamiento"
        android:layout_width="384dp"
        android:layout_height="325dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="14dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="188dp"
        android:gravity="center"
        android:text="sdasdsda"
        android:textColor="#090909"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/dificultad"
        android:layout_width="344dp"
        android:layout_height="47dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="126dp"
        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:text="DIFICULTAD: Principiante"
        android:textColor="#000000"
        android:textSize="23sp" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="82dp"
        android:layout_height="68dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="90dp"
        android:layout_marginRight="90dp"
        android:layout_marginBottom="15dp"
        android:onClick="guardar_entrenamiento"
        app:srcCompat="@drawable/guardar" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="71dp"
        android:layout_height="55dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="20dp"
        android:onClick="volver_menu"
        app:srcCompat="@drawable/volver" />

</RelativeLayout>